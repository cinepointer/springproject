<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
        
<mapper namespace="com.cinepointer.dao.tmdbDao">
	<insert id="insertGenre" parameterType="com.cinepointer.dto.tmdbGenreDto">
    INSERT INTO genre_table (genre_num, genre_name)
    VALUES (#{genreNum}, #{genreName});
	</insert>
</mapper>

        
